<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/main_theme"/>
            <corners
                android:bottomLeftRadius="64dp"
                android:topLeftRadius="64dp"
                android:bottomRightRadius="64dp"
                android:topRightRadius="64dp"/>
        </shape>
    </item>
    <item android:top="2dp" android:bottom="2dp" android:start="2dp" android:end="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners
                android:bottomLeftRadius="64dp"
                android:topLeftRadius="64dp"
                android:bottomRightRadius="64dp"
                android:topRightRadius="64dp"/>
        </shape>
    </item>
</layer-list>